{
	"name": "simplesphere",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prepare": "husky",
		"prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
		"prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
	},
	"lint-staged": {
		"src/**/*.{jsx,js,ts,tsx}": [
			"eslint --cache --fix",
			"prettier --write"
		],
		"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"next": "15.0.3",
		"react": "19.0.0-rc-66855b96-20241106",
		"react-dom": "19.0.0-rc-66855b96-20241106"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.6.0",
		"@commitlint/config-conventional": "^19.6.0",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"eslint": "^8",
		"eslint-config-next": "15.0.3",
		"husky": "^9.1.7",
		"lint-staged": "^15.2.10",
		"postcss": "^8",
		"prettier": "^3.4.1",
		"tailwindcss": "^3.4.1",
		"typescript": "^5"
	}
}